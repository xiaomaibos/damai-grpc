<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.ecnu.damai.dao.mapper.CategoryMapper">
    <select id="getCategoryList" resultType="cn.ecnu.damai.entity.Category">
        SELECT id AS cid,
               name,
               count
        FROM category
    </select>
</mapper>