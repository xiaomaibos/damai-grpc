<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.ecnu.damai.dao.mapper.CityMapper">
    <select id="getCityList" resultType="cn.ecnu.damai.entity.City">
        SELECT id AS cityId,
               name,
               code,
               count
        FROM city
    </select>
</mapper>